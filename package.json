{
  "name": "nuxt-version-checker",
  "version": "2.0.2",
  "description": "A minimal Nuxt 3 module that detects if a new version of your app is available and fetches a remote changelog in Markdown format.",
  "repository": "rtrap-rsi/nuxt-version-checker",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.5.0",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.5",
    "@nuxt/test-utils": "^3.19.1",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.29.0",
    "nuxt": "^3.17.5",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  }
}
